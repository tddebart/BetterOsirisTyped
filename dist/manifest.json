{
    "manifest_version": 2,
    "name": "Chrome Extension Starter",
    "description": "A Chrome Extension starter kit",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["*://davinci.osiris-student.nl/*"],
            "js": ["js/main.js"]
        }
    ],
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "permissions": ["tabs", "activeTab", "notifications", "http://*/", "https://*/"]
}
